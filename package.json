{
	"name": "indexerator",
	"displayName": "indexerator",
	"description": "This vscode extension create index file in current directory to export everything from it",
	"publisher": "hemo",
	"version": "1.1.1",
	"engines": {
		"vscode": "^1.30.0"
	},
	"homepage": "https://github.com/aviadhhh/indexerator",
	"repository": {
		"type": "git",
		"url": "https://github.com/aviadhhh/indexerator"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.indexerator"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.indexerator",
				"title": "indexerator"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "extension.indexerator",
					"when": "explorerResourceIsFolder",
					"group": "indexerator@1"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "yarn run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^8.10.25",
		"lint-staged": "^8.1.0",
		"tslint": "^5.8.0",
		"typescript": "^3.1.4",
		"vscode": "^1.1.25"
	}
}
